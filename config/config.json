{
  "parametrised": true,
  "sysTitle": "ION Datamining",
  "di": {
    "reportMeta": {
      "module": "@iondv/report/lib/impl/DsReportMetaRepository",
      "initMethod": "init",
      "initLevel": 1,
      "options": {
        "dataSource": "ion://Db",
        "calc": "ion://calculator"
      }
    },
    "mineBuilder": {
      "module": "@iondv/report/lib/impl/StdMineBuilder",
      "options": {
        "dataSource": "ion://Db",
        "metaRepo": "ion://metaRepo",
        "dataRepo": "ion://dataRepo"
      }
    },
    "reportBuilder": {
      "module": "@iondv/report/lib/impl/StdReportBuilder",
      "options": {
        "dataSource": "ion://Db",
        "metaRepo": "ion://metaRepo",
        "dataRepo": "ion://dataRepo"
      }
    },
    "reportExporter": {
      "module": "@iondv/report/backend/exporter",
      "initMethod": "init",
      "initLevel": 2,
      "options": {
        "exportPath": "[[report.exportPath]]",
        "log": "ion://sysLog",
        "bg": "ion://background",
        "reportMeta": "ion://reportMeta",
        "reportBuilder": "ion://reportBuilder"
      }
    }
  }
}
